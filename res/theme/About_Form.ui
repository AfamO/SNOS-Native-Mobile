      </component>
      <component type="Button" name="AboutContactsBut" cloudDestinationProperty="AboutContactsBut" uiid="ToolbarButton" flatten="false" text="" icon="contact_pix.png" >
      </component>
      <component type="Button" name="AboutSettingBut" cloudDestinationProperty="AboutSettingBut" uiid="ToolbarButton" flatten="false" text="" icon="setting_pix.png" >
      </component>
    </component>
  </component>
